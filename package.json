{
  "name": "kart-js",
  "version": "1.0.0-alpha.4",
  "description": "A JavaScript implmentation of Kart: Distributed version-control for geospatial and tabular data",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/entry-node.d.ts",
      "import": "./dist/dist/node/esm/index.js",
      "default": "./dist/dist/node/cjs/index.js"
    },
    "./browser": {
      "types": "./dist/entry-browser.d.ts",
      "default": "./dist/dist/browser/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node src/index.ts",
    "build": "node build.ts",
    "postinstall": "patch-package",
    "release": "npm run build && release-it --preRelease=alpha"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.28.2",
  "dependencies": {
    "@msgpack/msgpack": "^3.1.3",
    "@ngageoint/geopackage": "^5.0.2-beta",
    "@ngageoint/simple-features-geojson-js": "^1.1.1",
    "@noble/hashes": "^2.0.1",
    "@rollup/plugin-alias": "^6.0.0",
    "@ungap/raw-json": "^0.4.4",
    "@zenfs/core": "^2.4.4",
    "@zenfs/dom": "^1.2.7",
    "component-emitter": "^2.0.0",
    "decimal.js": "^10.6.0",
    "devalue": "^5.6.2",
    "es-arraybuffer-base64": "^1.1.2",
    "isomorphic-git": "^1.36.3",
    "p-limit": "^7.3.0",
    "proj4": "^2.20.2",
    "temporal-polyfill": "^0.3.0",
    "wkt-parser": "^1.5.2",
    "zod": "^4.3.6",
    "zod-geojson": "^1.6.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-replace": "^6.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/geojson": "^7946.0.16",
    "@types/node": "^25.2.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "patch-package": "^8.0.1",
    "release-it": "^19.2.4",
    "rollup": "^4.57.1",
    "stream": "^0.0.3",
    "typescript": "^5.9.3",
    "util": "^0.12.5"
  }
}
